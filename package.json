{
	"name": "date-picker",
	"version": "1.0.2",
	"description": "Pick dates using a calendar view in Obsidian.",
	"main": "main.js",
	"packageManager": "bun@1.2.22",
	"scripts": {
		"dev": "bun run --watch build.ts",
		"build": "bun run build.ts --prod",
		"version": "bun version-bump.ts",
		"check": "biome check && tsc --noEmit",
		"fix": "biome check --write"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@codemirror/language": "^6.11.3",
		"@types/bun": "latest",
		"obsidian": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"private": true,
	"license": "MIT",
	"dependencies": {
		"@codemirror/state": "^6.5.2",
		"@codemirror/view": "^6.38.6"
	}
}
